{
  "name": "@south-paw/discord-music-bot",
  "version": "0.0.1",
  "description": "A music bot for Discord servers, self-hosted, easy to use and extendable.",
  "keywords": [
    "discord",
    "bot",
    "music",
    "musicbot",
    "radio"
  ],
  "homepage": "https://github.com/South-Paw/discord-music-bot",
  "bugs": "https://github.com/South-Paw/discord-music-bot/issues",
  "license": "GPL-3.0",
  "author": {
    "name": "Alex Gabites",
    "email": "hello@southpaw.co.nz",
    "url": "http://southpaw.co.nz/"
  },
  "files": [
    "bin",
    "src"
  ],
  "bin": {
    "discord-music-bot": "./bin/cli.js"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/South-Paw/discord-music-bot"
  },
  "scripts": {
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn ts:watch\" \"yarn start:watch\"",
    "watch:debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn ts:watch\" \"yarn start:debug\"",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "build": "yarn clean && yarn ts && yarn lint",
    "start": "node dist/start.js",
    "start:watch": "nodemon dist/start.js | pino-pretty -c -l -t",
    "start:debug": "nodemon --inspect dist/start.js | pino-pretty -c -l -t",
    "ts": "tsc",
    "ts:watch": "tsc -w",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "discord.js": "^12.2.0",
    "meow": "^7.0.1",
    "pino": "^6.3.1",
    "string-format": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/pino": "^6.0.1",
    "@types/string-format": "^2.0.0",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "nodemon": "^2.0.3",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=13"
  },
  "publishConfig": {
    "access": "public"
  }
}
